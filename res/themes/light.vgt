     @theme = Light

      panel = [ #ebebeb, #e1e1e1 ]
      title = #000
       mast = #f9a806
      swash = #000
       logo = #000
        sep = #888
     symbol = #000
      patty = #222
patty-hover = hsl(216, .5, .5)
 info-glyph = hsl(0,0%,25%)

// knobs
k-bg      = [ #dfeffc, #bfbfbf ]
k-inner   = none stroke none
k-mark    = #cedcf2 opacity .8
k-pin     = stroke #457aa6
