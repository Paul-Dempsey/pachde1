// text buttons
tbtn-punch = none stroke #606060 width .75
tbtn-bevel = [ #fff, #666666]
tbtn-face = [ #e6e6e6, #bbb]
tbtn-text = #000
tbtn-text-dn = #666666
tbtn-hover = stroke #1a1a1a width 1.35

// Coppers
copper-title = #000
  medallion = #f9a54b stroke #f9a54b
  picker-bg = #888
     out-bg = #888 stroke none
// Copper mini
      i-sep = #cccccc
      o-sep = #cccccc
     lab-in = #888
    lab-out = #888
  minilabel = #bfbfbf
mini-out-bg = none

// Skiff
jack-glyph = #dedede stroke #000
//unscrew = #dedede stroke #000
