// Rail theme
@theme = Light
rail-blank       = #aaa
rail-gbg         = [ #ebebeb, #e0e0e0 ]
rail-bg          = #ccc
plain-rail       = #efefef
top-rail         = #efefef
top-rail-top     = #555
bottom-rail      = #efefef
bottom-rail-top  = #444
rail-hole        = #1f1f1f
pinstripe        = none stroke #ddd width 5
pin-rail         = #ddd
pin-rail-top     = #f1f1f1
pin-bot-rail-top = #888
sine = stroke #999 width 3